{
  "name": "@iconis/icons",
  "version": "0.2.0",
  "description": "√çcones SVG inline (strings) para projetos web",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pdro-marqss/iconis-lib.git"
  },
  "homepage": "https://github.com/Pdro-marqss/iconis-lib#readme",
  "bugs": {
    "url": "https://github.com/Pdro-marqss/iconis-lib/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "icons",
    "svg",
    "inline-svg",
    "typescript",
    "angular",
    "ui",
    "design",
    "tree-shaking",
    "web-icons"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./outlined": {
      "types": "./dist/outlined.d.ts",
      "import": "./dist/outlined.js",
      "require": "./dist/outlined.cjs"
    },
    "./filled": {
      "types": "./dist/filled.d.ts",
      "import": "./dist/filled.js",
      "require": "./dist/filled.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/outlined.ts src/filled.ts --format esm,cjs --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
