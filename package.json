{
  "name": "@iconis/icons",
  "version": "0.1.1",
  "description": "√çcones SVG inline (strings) para projetos web",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pdro-marqss/iconis-lib.git"
  },
  "homepage": "https://github.com/Pdro-marqss/iconis-lib.git#readme",
  "bugs": {
    "url": "https://github.com/Pdro-marqss/iconis-lib/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
